<html>
    <head>
        <title> Hello World </title>
        <script>
            function Hello(){
                var a= "Hello  World!!!";
                console.log(a);
                document.getElementById("Hello").value;
                document.getElementById("h").innerHTML=a;
                window.alert(a);
            }
        </script>
    </head>
    <body><center>
        <input type="button" id="Hello" value="Hello World!!!" onclick="Hello()">
        <p id="h"></p>
    </center></body>
