package PracticePrograms;

public class PatternsPractice {

	public static void main(String[] args) {
		for(int i=1;i<=4;i++) {
			for(int k=0;k<=i;k++) {
				System.out.print(" ");
			}
			for(int j=4;j<=4;j--) {
				System.out.print("*");
			}
		}System.out.println();
		// TODO Auto-generated method stub

	}

}
