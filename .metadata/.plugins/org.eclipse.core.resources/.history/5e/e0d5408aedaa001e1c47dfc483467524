package Collections;
import java.util.*;   
//public class ComparetoMethImplementObj {
   public class ComparetoMethImplementObj implements Comparable<ComparetoMethImplementObj>{    
    int id;    
    String name,author,publisher;    
    int quantity;    
    public ComparetoMethImplementObj(int id, String name, String author, String publisher, int quantity) {    
        this.id = id;    
        this.name = name;    
        this.author = author;    
        this.publisher = publisher;    
        this.quantity = quantity;    
    }    
    // implementing the abstract method  
    public int compareTo(ComparetoMethImplementObj b) {    
        if(id>b.id){    
            return 1;    
        }else if(id<b.id){    
            return -1;    
        }else{    
        return 0;    
        }    
    }    
    }    
    class TreeSetExample {    
    public static void main(String[] args) {    
        Set<ComparetoMethImplementObj> set=new TreeSet<ComparetoMethImplementObj>();    
        //Creating Books    
        ComparetoMethImplementObj b1=new ComparetoMethImplementObj(1, "Java", "James Gosling", "James",200);  
        ComparetoMethImplementObj b2=new ComparetoMethImplementObj(2, "Python", "Guido van Russom","Russom",150);    
        ComparetoMethImplementObj b3=new ComparetoMethImplementObj(3, "HTML", "Berner's Lee", "Lee", 100);    
        //Adding Books to TreeSet    
        set.add(b1);    
        set.add(b2);    
        set.add(b3);    
        //Traversing TreeSet    
        for(ComparetoMethImplementObj b:set){    
        System.out.println(b.id+" "+b.name+" "+b.author+" "+b.publisher+" "+b.quantity);    
        }    
    }    
    }
 //   }

